<% if (theme.socialLink.github) { %>
    <a href="<%= theme.socialLink.github %>" class="tooltipped" target="_blank" data-tooltip="访问我的GitHub" data-position="top" data-delay="50">
        <i class="fa fa-github"></i>
    </a>
<% } %>

<% if (theme.socialLink.email) { %>
    <a href="mailto:<%= theme.socialLink.email %>" class="tooltipped" target="_blank" data-tooltip="邮件联系我" data-position="top" data-delay="50">
        <i class="fa fa-envelope-open"></i>
    </a>
<% } %>

<% if (theme.socialLink.qq) { %>
    <a href="tencent://AddContact/?fromId=50&fromSubId=1&subcmd=all&uin=<%= theme.socialLink.qq %>" class="tooltipped" data-tooltip="QQ联系我: <%= theme.socialLink.qq %>" data-position="top" data-delay="50">
        <i class="fa fa-qq"></i>
    </a>
<% } %>

<% if (theme.socialLink.qqgroup) { %>
    <a href="http://shang.qq.com/wpa/qunwpa?idkey=<%= theme.socialLink.qqgroupkey %>" class="tooltipped" data-tooltip="加入QQ群: <%= theme.socialLink.qqgroup %>" data-position="top" data-delay="50">
	<i class="fa fa-qq"></i>
    </a>
<% } %>

<% if (theme.socialLink.weibo) { %>
    <a href="<%= theme.socialLink.weibo %>" class="tooltipped" target="_blank" data-tooltip="访问我的微博" data-position="top" data-delay="50">
        <i class="fa fa-weibo"></i>
    </a>
<% } %>


<% if (theme.socialLink.facebook) { %>
    <a href="<%= theme.socialLink.facebook %>" class="tooltipped" target="_blank" data-tooltip="访问我的facebook" data-position="top" data-delay="50">
        <i class="fa fa-facebook"></i>
    </a>
<% } %>

<% if (theme.socialLink.twitter) { %>
    <a href="<%= theme.socialLink.twitter %>" class="tooltipped" target="_blank" data-tooltip="访问我的Twitter" data-position="top" data-delay="50">
        <i class="fa fa-twitter"></i>
    </a>
<% } %>

<% if (theme.socialLink.about) { %>
    <a href="<%= theme.socialLink.about %>" class="tooltipped" target="_blank" data-tooltip="关于我" data-position="top" data-delay="50">
        <i class="fa fa-user"></i>
    </a>
<% } %>

<% if (config.feed && config.feed.path == 'atom.xml') { %>
    <a href="<%- url_for('/atom.xml') %>" class="tooltipped" target="_blank" data-tooltip="RSS 订阅" data-position="top" data-delay="50">
        <i class="fa fa-rss"></i>
    </a>
<% } %>